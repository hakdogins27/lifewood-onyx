{
  "version": 2,
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/index.py"
    },
    {
      "src": "/(.*\\.(css|js|ico|png|jpg|jpeg|gif|svg))$",
      "dest": "/public/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1.html"
    }
  ]
}